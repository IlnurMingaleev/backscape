.content {
    flex-grow: 1;
    overflow: visible;
    visibility: visible;
    opacity: 0.28;
    width: 100%;
    max-height: none;
    min-height: auto;
    flex-shrink: 1;
    flex-direction: row;
    flex-wrap: wrap;
    align-items: stretch;
    justify-content: center;
    flex-basis: auto;
    top: 0;
    height: 100%;
    scale: 1 1;
    transition-property: scale;
    transition-duration: 3s;
    transition-delay: 0.1s;
    transition-timing-function: ease-in-out;
}

.margin_left_scrollview {
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
