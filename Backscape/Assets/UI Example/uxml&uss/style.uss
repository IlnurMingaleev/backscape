}

.scrollview {
    transition-property: margin-left;
    transition-duration: 1s;
    transition-timing-function: ease-in-out;
}

.unity-scroll-view__content-viewport {
    flex-basis: auto;
    width: 100%;
    height: 100%;
    overflow: hidden;
}
